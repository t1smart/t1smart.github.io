/**
 * @file
 * A JavaScript file for the theme.
 *
 * In order for this JavaScript to be loaded on pages, see the instructions in
 * the README.txt next to this file.
 */

// JavaScript should be made compatible with libraries other than jQuery by
// wrapping it with an "anonymous closure". See:
// - https://drupal.org/node/1446420
// - http://www.adequatelygood.com/2010/3/JavaScript-Module-Pattern-In-Depth

// scroll top if url contains hash
// to animate the scroll later

if (typeof scrolledToHash === 'undefined') {
  scrolledToHash = false;
}

// we remove the hash temporarily
// to prevent the page from jumping

if (location.hash && !scrolledToHash) {
  var locationHashContainer = location.hash;
  location.hash = '';
  setTimeout(function() {
    window.scrollTo(0, 0);
  }, 1);
}


(function ($) {


// To understand behaviors, see https://drupal.org/node/756722#behaviors
  Drupal.behaviors.trpdc_custom_theme = {
    attach: function(context, settings) {

      $(".flag-watchlists a.unflag-action").mouseover(function () {
        $(this).find('.watch-text-link').text("Remove from watch list");
      });
      $(".flag-watchlists a.unflag-action").mouseout(function () {
        $(this).find('.watch-text-link').text("Watching");
      });

      $('.hidden').hide();

      $('body').on("click", '.downloadable-item-button:not(.is-disabled)', function(e) {
        // Prevent default flow
        e.preventDefault();

        var url = $(this).parent('a').attr('href');

        var nid = $(this).data('nid');
        
        if (typeof nid == 'undefined') {
          return;
        }
  
        var data = {
          nid: nid,
        };
        // create audit data
        $.ajax('/download_audit', { data: data }).done(function() {
          if (typeof url != 'undefined' && url.length > 0) {
            location.href = url;
          }
        });
      });

      $(".plus-minus-toggle").click(function () {
        if ($(".item-list", $(this).parent().parent()).is(":visible")) {
          $(".item-list", $(this).removeClass("contract-icon").parent().parent()).hide();
        } else {
          $(".item-list", $(this).addClass("contract-icon").parent().parent()).show();
        }
        return false;
      });
      $(".contract-icon").parent().parent().find(".item-list").show();
      $(".block-facetapi a.facetapi-checkbox").show();

    }
  };

  // Match height for carousel items.

  Drupal.behaviors.trpdc_schwinn_matchHeight = {
    attach: function(context, settings) {
      $('.carousel-inner > .item', context).matchHeight({
        byRow: false
      });
      $('a.views-row-content > div, a.search-result > div', context).matchHeight({
        byRow: false
      });
    }
  };

  // Scroll tabs
  // when they don't fit the container's width

  Drupal.behaviors.trpdc_schwinn_scrollableTabs = {
    attach: function(context, settings) {
      var $container = $('.tabs', context);
      var $list = $('.nav-tabs', $container);
      if ($container.length && $list.length) {
        Drupal.trpdc_schwinn.scrollTabResponsive($container, $list);
        Drupal.trpdc_schwinn.scrollTabActions($container, $list);
        $(window).resize($.throttle( 250, function() {
          Drupal.trpdc_schwinn.scrollTabResponsive($container, $list);
        }));
      }
    }
  };

  // Admin menus.
  // Adjustments.

  Drupal.behaviors.trpdc_schwinn_adminMenus = {
    attach: function(context, settings) {
      var $window = $(window);
      var $body = $('body');
      $window.on('scroll resize', $.throttle(250, function(){
        var scrollPos = $window.scrollTop();
        if (scrollPos > 0) {
          $body.not('scrolled').addClass('scrolled');
        } else {
          $body.removeClass('scrolled');
        }
      }));
    }
  };

  // Scroll to hash

  Drupal.behaviors.trpdc_schwinn_scrollToHash = {
    attach: function(context, settings) {
      if (typeof locationHashContainer === 'undefined') return;
      if (locationHashContainer.length) {
        $(window).load(function(){
          Drupal.trpdc_schwinn.scrollToHash(locationHashContainer);
        });
      }
      $.ajax({
        complete: function(){
          if (locationHashContainer.length) {
            Drupal.trpdc_schwinn.scrollToHash(locationHashContainer);
          }
        }
      });
    }
  };

  // Ellipsis at the end of the overflowed text

  Drupal.behaviors.trpdc_schwinn_ellipsis = {
    attach: function(context, settings) {
      var $snippets = $('.summary-offset, .api-summary, .search-snippet, .row-taxonomy-wrapper', context)
        .not('.api-summary .summary-offset');
      $snippets = $snippets.add('.tr-content-page-api-family-teaser .summary', context);
      $snippets = $snippets.add('.view-tr-content-pages .views-field-field-overview .field-content', context);
      $(window).load(function(){
        $snippets.dotdotdot();
      });
    }
  };


  // Helper functions

  Drupal.trpdc_schwinn = Drupal.trpdc_schwinn || {};

  // ScrollTabs helpers

  Drupal.trpdc_schwinn.scrollTabResponsive = function($container, $list) {
    if ($container.width() < $list.width()) {
      $container
        .addClass('scroll-right')
        .once('scrollable-wrapper', function(){
          var $_scrollableWrapper = $('<div/>', {
            'class': 'scrollable-wrapper'
          });
          $list.wrap($_scrollableWrapper);
        })
    } else {
      $container
        .removeClass('scroll-right scroll-left');
    }
  };

  Drupal.trpdc_schwinn.scrollTabActions = function($container, $list) {
    $container.on('click', function(ev){
      var $wrapper = $('.scrollable-wrapper', this);
      var offset = $list.width() - $wrapper.width() + 43;
      if ($(ev.target).is('.tabs')) {
        var width = ev.target.offsetWidth;
        if (ev.offsetX > width/2) {
          $container.addClass('scroll-left');
          $wrapper.animate({'scrollLeft':  offset}, 300);
        } else {
          $container.removeClass('scroll-left');
          $wrapper.animate({'scrollLeft': 0}, 300);
        }
      }
    })
  };

  // ScrollToHash helpers

  Drupal.trpdc_schwinn.getHeaderOffset = function(){

    // check sticky header's height
    // to take it into account when scrolling

    var $header = $('.site-header');
    var position = $header.position().top;
    var height = $header.outerHeight();
    var offset = position + height;
    return offset;
  };

  Drupal.trpdc_schwinn.scrollToTarget = function($target) {

    // jump to the top of the page
    // and then scroll smoothly to the 
    // appropriate position

    if ($target.length < 1) return;
    var $screen = $('html, body');
    var position = $target.offset().top;
    position -= Drupal.trpdc_schwinn.getHeaderOffset();

    $screen
      .animate({'scrollTop': position}, 'slow', 'swing', function(){

        // check header height again and adjust position

        position = $target.offset().top;
        position -= Drupal.trpdc_schwinn.getHeaderOffset();
        position -= 20;
        $screen
          .animate({'scrollTop': position}, 1000);
      });

  };

  Drupal.trpdc_schwinn.scrollToHash = function(hash) {
    if (scrolledToHash || typeof hash == 'undefined' || hash.length < 1) return;
    var selector = hash.slice(1);
    var $target = $('#' + selector + ', [name="' + selector + '"]');
    if ($target.length < 1) {
      return;
    } else {
      Drupal.trpdc_schwinn.scrollToTarget($target);

      // put the hash back to the url
      // but without page jumping

      if(history.pushState) {
        history.pushState(null, null, hash);
      } else {
        location.hash = hash;
      }
      scrolledToHash = true;
    }
  };

})(jQuery);;
(function ($, Drupal, window, document, undefined) {
    Drupal.behaviors.ui_form_select = {
        attach: function (context, settings) {
            var $containers = $(".view-all-api-families .top-widgets, .view-display-id-source_code_list .top-widgets, .api-overview-title, .add-api-product, .profile-notification-settings");
            $containers.find('select.form-select').each(function () {
                $(this).once("ui-form-select", function () {
                    var $this = $(this),
                        numberOfOptions = $this.children('option').length;

                    $this.addClass('ui-form-select_hidden');
                    $this.wrap('<div class="ui-form-select"></div>');
                    $this.after('<div class="ui-form-select-styled"></div>');

                    var $styledSelect = $this.next('div.ui-form-select-styled');
                    var $selectedOption = $this.children(":selected").eq(0);
                    if (!$selectedOption.length) {
                        $selectedOption = $this.children('option').eq(0);
                        $this.children('option').eq(0).attr('selected','selected');
                    }

                    var $wrappedtext = $selectedOption.text().replace(/\(.*?\)/g, '<span class="terms-count">$&</span>');

                    $styledSelect.html($wrappedtext);

                    var $list = $('<ul />', {
                        'class': 'ui-form-select-options'
                    }).insertAfter($styledSelect);

                    for (var i = 0; i < numberOfOptions; i++) {
                        var $wrappedtext = $this.children('option').eq(i).text().replace(/\(.*?\)/g, '<span class="terms-count">$&</span>');
                        var $liClass = '';
                        $count = $this.children('option').eq(i).text().match(/\((.*)\)/);
                        if($count != null && $count[1] == "0") {
                            $liClass="disabled";
                        }

                        $('<li />', {
                            rel: $this.children('option').eq(i).val(),
                            html: $wrappedtext,
                            class: $liClass,
                        }).appendTo($list);
                    }

                    var $listItems = $list.children('li');

                    $styledSelect.click(function (e) {
                        e.stopPropagation();
                        $(this).toggleClass('active').next('ul.ui-form-select-options').toggle();
                        return false;
                    });

                    $listItems.click(function (e) {
                        if($(this).hasClass('disabled')) {
                            return false;
                        }
                        e.stopPropagation();
                        $styledSelect.html($(this).html()).removeClass('active');
                        $this.val($(this).attr('rel'));
                        $list.hide();
                        $this.trigger("change");

                        return false;
                    });

                    $(document).click(function () {
                        $styledSelect.removeClass('active');
                        $list.hide();
                    });
                });
            });
        }
    };
})(jQuery, Drupal, this, this.document);
;
(function ($, Drupal, window, document, undefined) {
  Drupal.behaviors.ui_checkbox = {
    attach: function(context, settings) {
      $('input[type="checkbox"]').each( function() {
        // Apply jquery once to avoid adding classes on every AJAX call in the page. For more info https://www.drupal.org/node/756722
        $(this).once("ui-checkbox-processed", function(){
          var $checkbox = $(this);
          var $label = $("label[for='"+$(this).attr("id")+"']");
          var $uiCheckbox = $("<span>").addClass("ui-checkbox");

          $uiCheckbox.insertAfter($checkbox);
          setUiCheckboxState($uiCheckbox, $checkbox);
          $checkbox.hide();

          $uiCheckbox.add($label).on("click", function() {
            if($checkbox.is(":checked")){
              $checkbox.prop('checked', false);
            }
            else{
              $checkbox.prop('checked', true);
            }
            
            setUiCheckboxState($uiCheckbox, $checkbox);

            //triggering the 'change' event for the facet's on the search page does not work        
            if($checkbox.hasClass('facetapi-checkbox')) {
              $checkbox.trigger('click');
            } else {
              $checkbox.trigger('change');
            }
            
            return false;
          });
        });
      });

      function setUiCheckboxState($uiCheckbox, checkbox) {
        var checkedClass = "icon-checkbox-checked2",
            uncheckedClass = "icon-checkbox-unchecked2";
            
        if(checkbox.is(":checked")) {
          $uiCheckbox.removeClass(uncheckedClass);
          $uiCheckbox.addClass(checkedClass);
        } else {
          $uiCheckbox.removeClass(checkedClass);
          $uiCheckbox.addClass(uncheckedClass);
        }
      }
    }
  };
})(jQuery, Drupal, this, this.document);;
