/**
 * @file
 * Custom behavior to open and close a dropdown menu.
 *
 * In order for this JavaScript to be loaded on pages, see the instructions in
 * the README.txt next to this file.
 */

// JavaScript should be made compatible with libraries other than jQuery by
// wrapping it with an "anonymous closure". See:
// - https://drupal.org/node/1446420
// - http://www.adequatelygood.com/2010/3/JavaScript-Module-Pattern-In-Depth
(function ($, Drupal, window, document, undefined) {


  // To understand behaviors, see https://drupal.org/node/756722#behaviors
  Drupal.behaviors.trpdc_dropdown_menu = {
    attach: function(context, settings) {
      // Find the dropdown trigger.
      var trigger = $('.header-dropdown__trigger');

      // Attach a click event to toggle open and close the dropdown menu.
      trigger.on('click', function(e) {
      // Prevent the link from it's default action.

        e.preventDefault();
        // Find the trigger's menu.
        var menu = $(this).siblings('.header-dropdown__list');
        // Toggle open/close the menu.
        menu.toggle();
        if(menu.is(':hidden')){
    	    menu.parent().removeClass('open');
        } else {
    	    menu.parent().addClass('open');
        }
      });

      // Close if clicked outside the menu trigger.
      $(document).mouseup(function (e){
        var container = $(".header-dropdown__trigger");
        if (!container.is(e.target) // if the target of the click isn't the container...
            && container.has(e.target).length === 0) // ... nor a descendant of the container
        {
          // Find the trigger's menu.
          var menu = container.siblings('.header-dropdown__list');
          // Toggle open/close the menu.
          menu.hide();
  	      menu.parent().removeClass('open');
        }
      });


      // slick menu changes
      var isTop = true;
      var $elemsToHide = $(".site-header").children("section").not(".trpdc-header");
      var slickMenu = false;
      var $win = $(window);

      $win.on( 'scroll', function() {

        if ($win.scrollTop() > 0) {
          if (isTop) {
            isTop = false;
            $elemsToHide.stop().slideUp();
            if( $('.slicknav_menu').is(":visible") ) {
              // $('.slicknav_menu').slideUp();
              slickMenu = true;
            }
          }
        } else if (!isTop) {
          isTop = true;
          $elemsToHide.slideDown();
          if(slickMenu) {
            // $('.slicknav_menu').slideDown();
          }
        }
      });
    }
  };
})(jQuery, Drupal, this, this.document);;
