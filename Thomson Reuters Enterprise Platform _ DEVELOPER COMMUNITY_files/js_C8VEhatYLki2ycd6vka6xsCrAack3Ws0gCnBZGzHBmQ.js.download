(function ($) {
  Drupal.behaviors.ContactRating = {
    attach: function (context, settings) {
      var score = $('input[name="user_rating"]').val();
      $('.5star-contact').raty({
        path: 'sites/all/modules/custom/tr_contact/img',
        //cancel: true,
        score: score,
        target: '.hint-rating',
        targetType : 'number',
        targetKeep : true,
        click : function(score, evt) {
