(function ($) {
  Drupal.behaviors.scrollTo = {
    attach: function(context, settings) {

      var offset = 0;

      //add header height
      offset += $('.trpdc-header').height();

      // hack~ish but can't get the admin-menu's height at this point
      // and it should always be 21px unless we update the module.
      if (settings.admin_menu) {
        offset += 29;
      }

      function scrollToSelector(selector, event) {
        event.preventDefault();
        var $this = $(selector);
        if (typeof $this.offset() != 'undefined') {
          $('html, body').animate({
            scrollTop: $this.offset().top - offset
          }, 500, 'swing');
          // Get link
          var link = $this.attr('href');
          // Get the page title
          var title = $this.text();
          // Save history
          window.history.pushState('obj', title, link);
        }
      }

      $('[data-scroll-to]', context).click(function(e) {
        scrollToSelector($(this).attr('data-scroll-to'), e)
      });

      var $tutorialViewport = $("#tutorial-viewport");
      if($tutorialViewport.length) {
        $tutorialViewport.on("click", 'a[href^="#"]', function(e) {
          scrollToSelector($(this).attr('href'), e)
        })
      } else {
        $('a[href^="#"]', context).click(function(e) {
          var link = $(this).attr('href');
          if(link.length < 2) {
            return;
          }
          scrollToSelector(link, e)
        });
      }
    }
  };
})(jQuery);;
