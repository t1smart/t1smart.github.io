(function ($) {
  Drupal.behaviors.trpdc_traac = {
    attach: function (context, settings) {
      if (typeof TRAAC != 'undefined') {

        //Track page view in general, with info generated in the module
        traac_helper.kvpTrack("page viewed", Drupal.settings.traac_kvp_info)

        //If KVP data but no event, store data for parent event
        // format like <span data-event-kvp='{"Action":"Some Action"}'>
        $("*[data-event-kvp]").on("click submit", function(e) {
          var $this = $(e.currentTarget);
          if ($this.data("event") || $this.data("event-submit")) {
            //let the data-event listener handle it
            return;
          }
          if (!e.originalEvent.eventKvp) {
            e.originalEvent.eventKvp = {};
          }
          var dataTargetKvp = $this.data("event-kvp");
          for (var key in dataTargetKvp) {
            e.originalEvent.eventKvp[key] = dataTargetKvp[key];
          }
        });

        //Capture based on data-event attributes
        $("*[data-event]").on("click", function(e) {
          trackEvent(e);
        });
        $("*[data-event-submit]").on("submit", function(e) {
          trackEvent(e);
        });
        //Track all clicks and submits that bubble to the body without getting captured
        $("body").on("click submit traac", function(e) {
          trackEvent(e);
        });

        function trackEvent(e) {

          // Return if the event was triggered manually by code, unless traac event
          if(typeof e.originalEvent === 'undefined' && e.type != 'traac'){
            return;
          }

          //prevent event from getting tracked multiple times if nested data-events, without killing propagation
          if (e.type == 'traac') {
            if (e.traacdone) {
              return;
            } else {
              e.traacdone = true;
            }
          } else {
            if (e.originalEvent.traacdone) {
              return;
            } else {
              e.originalEvent.traacdone = true;
            }
          }

          var type = e.type.charAt(0).toUpperCase() + e.type.substr(1).toLowerCase();
          var kvp = traac_helper.kvpClone(Drupal.settings.traac_kvp_info);
          var $target = $(e.target);
          var $dataTarget = $(e.currentTarget);
          var dataTargetKvp = $dataTarget.data("event-kvp");
          var targetText = "";
          var eventName = e.eventName || $dataTarget.data("event") || $dataTarget.data("event-submit") || (type + " within page");
          if (e.originalEvent && e.originalEvent.eventKvp) {
            dataTargetKvp = $.extend(e.originalEvent.eventKvp, dataTargetKvp);
          }
          if (e.eventKvp) {
            dataTargetKvp = $.extend(e.eventKvp, dataTargetKvp);
          }

          //if event has required data, check that data exists before tracking
          //example format would be: data-event-requirement='["requiredKey", "2ndRequiredKey"]'
          // or just: data-event-requirement="requiredKey"
          if ($dataTarget.data("event-requirement")) {
            var reqs = $dataTarget.data("event-requirement");
            if (typeof reqs === 'string') {
              reqs = [reqs];
            }
            if (!Array.isArray(reqs)) {
              return;
            } else {
              var missingReq = false;
              $.each(reqs, function(i, req) {
                if (!(req in dataTargetKvp)) {
                  missingReq = true;
                }
              });
              if (missingReq) {
                return;
              }
            }
          }

          if (type == "Click") {
            targetText = $.trim($target.text());
            if ($target.closest('a').length) {
              kvp['ClickHref'] = $target.closest('a')[0].href;
            }
          }
          else if (type == "Submit" && $target.is('form')) {
            if ($target.prop("id")) {
              kvp['Form'] = $target.prop("id");
            } else {
              kvp['Form'] = e.target.classList.toString();
            }
            if ($target.find("input[type=text]").length == 1) {
              //if a single input, like the search form, use this for target text
              targetText = $target.find("input[type=text]").val();
            }
          }
          if (targetText.length) {
            kvp[type] = targetText.replace(/\s+/g, ' ');
          }
          for (var key in dataTargetKvp) {
            kvp[key] = dataTargetKvp[key];
          }
          kvp['TargetID'] = $target.prop("id");
          kvp['TargetClassList'] = e.target.classList.toString();
          kvp['TargetNodeType'] = e.target.tagName;
          traac_helper.kvpTrack(eventName, kvp);
        }

        function trackDownload(kvp, cb) {
          traac_helper.kvpTrack('File Downloaded', kvp);
          if(typeof cb === 'function') {
            cb();
          }
        }

        //catchall for download links
        $('#content').on('click', 'a', function(e) {
          var kvp = traac_helper.kvpClone(Drupal.settings.traac_kvp_info)
          var ending = /([^/]*)(\?|$)/.exec(this.href)[1];
          if (!ending) {
            return;
          }
          var extension = /[^.]*$/.exec(ending)[0];
          if(extension == ending || $.inArray(extension, ['com', 'htm', 'html']) > -1) {
            return;
          }
          kvp['Filename Downloaded'] = ending;
          trackDownload(kvp);
        });
      }
    }
  };
})(jQuery);
